    "version": "0.2.0",
    "configurations": [
        {
            "name": "gcc - Build and debug day01",
            "type": "cppdbg",
            "request": "launch",
            "program": "/home/alexander/Projects/C/aoc2021/target/day01",
            "args": [
                "./data/day01/test.txt"
            ],
            "stopAtEntry": false,
            "cwd": "/home/alexander/Projects/C/aoc2021",
            "environment": [],
            "externalConsole": false,
            "MIMode": "gdb",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                },
                {
                    "description": "Set Disassembly Flavor to Intel",
                    "text": "-gdb-set disassembly-flavor intel",
                    "ignoreFailures": true
                }
            ],
            "preLaunchTask": "build",
            "miDebuggerPath": "/usr/bin/gdb"
        },
